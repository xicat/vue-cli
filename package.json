{
  "name": "cli3",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint --mode production",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "vue": "^2.6.6"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "axios": "^0.18.0",
    "babel-core": "^6.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "echarts": "^4.2.1",
    "element-ui": "^2.7.2",
    "es6-promise": "^4.2.6",
    "eslint": "^5.8.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-vue": "^5.2.2",
    "eslint-plugin-vue-libs": "^3.0.0",
    "lint-staged": "^8.1.5",
    "mockjs": "^1.0.1-beta3",
    "prettier": "^1.16.4",
    "vue-i18n": "^8.10.0",
    "vue-router": "^3.0.2",
    "vue-template-compiler": "^2.5.21",
    "vuex": "^3.1.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "eslint --fix ",
      "git add"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
